// Compiler configuration to build the ES5 CommonJS bin files
{
  "extends": "./tsconfig.base.json",
  "exclude": ["../node_modules"],
  "include": ["../src/bin/*.ts"],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": false,
    "declarationMap": false
  }
}
